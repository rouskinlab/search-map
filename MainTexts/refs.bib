   journal = {Science},
   month = {7},
   pages = {199-204},
   publisher = {American Association for the Advancement of Science},
   title = {An overlapping protein-coding region in influenza A virus segment 3 modulates the host response},
   volume = {337},
   url = {https://www.science.org/doi/full/10.1126/science.1222213},
   year = {2012},
}
