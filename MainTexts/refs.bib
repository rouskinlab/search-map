   doi = {10.1093/NAR/GKAA1013},
   issn = {0305-1048},
   issue = {22},
   journal = {Nucleic Acids Research},
   keywords = {base pairing,diffuse mesangial sclerosis,dimethyl sulfide,genome,protein structure,protons,rna,sars-cov-2,secondary,spectrum analysis},
   month = {12},
