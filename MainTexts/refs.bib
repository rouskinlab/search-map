   keywords = {HPLC,In vitro transcription,Nucleoside modification,RNA},
   pages = {43-54},
   pmid = {23296926},
   publisher = {Humana Press Inc.},
   title = {HPLC purification of in vitro transcribed long RNA},
   volume = {969},
